<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="gameVM" type="com.toylysapplication.app.modules.game.data.viewmodel.GameVM"/>
    </data>
    <LinearLayout android:gravity="start" android:id="@+id/linearGame"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_A700">
        <androidx.appcompat.widget.Toolbar
            android:id="@+id/toolbarToolbar"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_30pxh"
            android:layout_marginStart="@dimen/_30pxh"
            android:layout_marginTop="@dimen/_37pxv"
            android:layout_width="match_parent" app:contentInsetStart="0dp">
            <LinearLayout android:gravity="start"
                android:id="@+id/linearBack"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageArrowleft"
                    android:layout_height="@dimen/_11pxh"
                    android:layout_marginBottom="@dimen/_7pxv"
                    android:layout_marginStart="@dimen/_4pxh"
                    android:layout_marginTop="@dimen/_7pxv"
                    android:layout_width="@dimen/_11pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_arrowleft"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_arrowleft"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtMotor"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_14pxh"
                    android:layout_weight="0.82"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{gameVM.gameModel.txtMotor}"
                    style="@style/txtPoppinsbold18" tools:text="@string/lbl_motor"/>
            </LinearLayout>
        </androidx.appcompat.widget.Toolbar>
        <LinearLayout android:id="@+id/linearUp"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_29pxh"
            android:layout_marginStart="@dimen/_29pxh"
            android:layout_marginTop="@dimen/_28pxv"
            android:layout_width="match_parent" android:orientation="horizontal">
            <TextView android:ellipsize="end"
                android:id="@+id/txtChooseYourMot"
                android:layout_height="wrap_content"
                android:layout_weight="0.88" android:layout_width="0dp"
                android:singleLine="true"
                android:text="@{gameVM.gameModel.txtChooseYourMot}"
                style="@style/txtPoppinsmedium24" tools:text="@string/msg_choose_your_mot"/>
            <ImageButton
                android:background="@drawable/rectangle_bg_bluegray_50_radius_5"
                android:id="@+id/btnSearch"
                android:layout_height="@dimen/_30pxh"
                android:layout_marginBottom="@dimen/_2pxv"
                android:layout_marginStart="@dimen/_97pxh"
                android:layout_marginTop="@dimen/_3pxv"
                android:layout_width="@dimen/_30pxh"
                android:paddingHorizontal="@dimen/_5pxh"
                android:paddingVertical="@dimen/_5pxv"
                android:scaleType="fitCenter"
                android:src="@drawable/img_search" tools:ignore="ContentDescription"/>
        </LinearLayout>
        <ImageView android:id="@+id/imageHondascoopymo"
            android:layout_height="@dimen/_119pxv"
            android:layout_marginTop="@dimen/_17pxv"
            android:layout_width="match_parent"
            android:scaleType="fitXY"
            imageRes="@{@drawable/img_hondascoopymo}"
            tools:ignore="ContentDescription" tools:src="@drawable/img_hondascoopymo"/>
        <FrameLayout android:id="@+id/frameStackfavorite"
            android:layout_gravity="center_horizontal"
            android:layout_height="@dimen/_259pxv"
            android:layout_marginBottom="@dimen/_278pxv"
            android:layout_marginEnd="@dimen/_27pxh"
            android:layout_marginStart="@dimen/_29pxh"
            android:layout_marginTop="@dimen/_53pxv" android:layout_width="@dimen/_358pxh">
            <LinearLayout android:id="@+id/linearColumnfavorite"
                android:layout_gravity="start"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginStart="@dimen/_1pxh"
                android:layout_width="match_parent" android:orientation="vertical">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtFavorite"
                    android:layout_gravity="start"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{gameVM.gameModel.txtFavorite}"
                    style="@style/txtPoppinsmedium18" tools:text="@string/lbl_favorite"/>
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearRowrevodxone"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_11pxh"
                    android:layout_marginStart="@dimen/_11pxh"
                    android:layout_marginTop="@dimen/_23pxv"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageRevodxOne"
                        android:layout_height="@dimen/_69pxh"
                        android:layout_width="@dimen/_63pxh"
                        android:scaleType="fitXY"
                        imageRes="@{@drawable/img_revodx1}"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_revodx1"/>
                    <ImageView android:id="@+id/imagePngtransparent"
                        android:layout_height="@dimen/_66pxh"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_marginStart="@dimen/_26pxh"
                        android:layout_marginTop="@dimen/_2pxv"
                        android:layout_width="@dimen/_68pxh"
                        android:scaleType="fitXY"
                        imageRes="@{@drawable/img_pngtransparent}"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_pngtransparent"/>
                    <ImageView android:id="@+id/imageHondavario125"
                        android:layout_height="@dimen/_64pxh"
                        android:layout_marginStart="@dimen/_18pxh"
                        android:layout_marginTop="@dimen/_5pxv"
                        android:layout_width="@dimen/_71pxh"
                        android:scaleType="fitXY"
                        imageRes="@{@drawable/img_hondavario125}"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_hondavario125"/>
                </LinearLayout>
                <LinearLayout android:gravity="end"
                    android:id="@+id/linearRowmotorone"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_7pxv"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtMotorOne"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_weight="0.24"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{gameVM.gameModel.txtMotorOne}"
                        style="@style/txtRobotomedium14" tools:text="@string/lbl_motor_1"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtMotorTwo"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_marginStart="@dimen/_36pxh"
                        android:layout_weight="0.25"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{gameVM.gameModel.txtMotorTwo}"
                        style="@style/txtRobotomedium14" tools:text="@string/lbl_motor_2"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtMotorThree"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_marginStart="@dimen/_33pxh"
                        android:layout_weight="0.25"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{gameVM.gameModel.txtMotorThree}"
                        style="@style/txtRobotomedium14" tools:text="@string/lbl_motor_3"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtMotorFour"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_35pxh"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_weight="0.25"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{gameVM.gameModel.txtMotorFour}"
                        style="@style/txtRobotomedium14" tools:text="@string/lbl_motor_4"/>
                </LinearLayout>
                <LinearLayout android:gravity="end"
                    android:id="@+id/linearRow2662733270mot"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_1pxh"
                    android:layout_marginStart="@dimen/_15pxh"
                    android:layout_marginTop="@dimen/_30pxv"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <LinearLayout
                        android:id="@+id/linearColumn2662733270mot"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.53"
                        android:layout_width="0dp" android:orientation="vertical">
                        <ImageView android:id="@+id/image2662733270Mot"
                            android:layout_gravity="end"
                            android:layout_height="@dimen/_67pxh"
                            android:layout_marginStart="@dimen/_10pxh"
                            android:layout_width="@dimen/_90pxh"
                            android:scaleType="fitXY"
                            imageRes="@{@drawable/img_2662733270mot}"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_2662733270mot"/>
                        <LinearLayout android:gravity="start"
                            android:id="@+id/linearRowmotorfive"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_10pxh"
                            android:layout_marginTop="@dimen/_1pxv"
                            android:layout_width="match_parent" android:orientation="horizontal">
                            <TextView android:ellipsize="end"
                                android:id="@+id/txtMotorFive"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="@dimen/_1pxv"
                                android:layout_weight="0.5"
                                android:layout_width="0dp"
                                android:singleLine="true"
                                android:text="@{gameVM.gameModel.txtMotorFive}"
                                style="@style/txtRobotomedium14" tools:text="@string/lbl_motor_5"/>
                            <TextView android:ellipsize="end"
                                android:id="@+id/txtMotorSix"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="@dimen/_1pxv"
                                android:layout_marginStart="@dimen/_34pxh"
                                android:layout_weight="0.5"
                                android:layout_width="0dp"
                                android:singleLine="true"
                                android:text="@{gameVM.gameModel.txtMotorSix}"
                                style="@style/txtRobotomedium14" tools:text="@string/lbl_motor_6"/>
                        </LinearLayout>
                    </LinearLayout>
                    <FrameLayout android:id="@+id/frameStackmotorseven"
                        android:layout_height="@dimen/_83pxv"
                        android:layout_marginStart="@dimen/_2pxh"
                        android:layout_marginTop="@dimen/_2pxv" android:layout_width="@dimen/_86pxh">
                        <TextView android:ellipsize="end"
                            android:gravity="top|start"
                            android:id="@+id/txtMotorSeven"
                            android:layout_gravity="bottom|center"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_15pxh"
                            android:layout_marginStart="@dimen/_15pxh"
                            android:layout_marginTop="@dimen/_10pxv"
                            android:layout_width="match_parent"
                            android:singleLine="true"
                            android:text="@{gameVM.gameModel.txtMotorSeven}"
                            style="@style/txtRobotomedium14" tools:text="@string/lbl_motor_7"/>
                        <ImageView android:id="@+id/imageHondahornetjpg"
                            android:layout_gravity="top|start"
                            android:layout_height="@dimen/_67pxv"
                            android:layout_marginBottom="@dimen/_10pxv"
                            android:layout_width="match_parent"
                            android:scaleType="fitXY"
                            imageRes="@{@drawable/img_hondahornetjpg}"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_hondahornetjpg"/>
                    </FrameLayout>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtMotorEight"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_marginStart="@dimen/_19pxh"
                        android:layout_marginTop="@dimen/_68pxv"
                        android:layout_weight="0.18"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{gameVM.gameModel.txtMotorEight}"
                        style="@style/txtRobotomedium14" tools:text="@string/lbl_motor_8"/>
                </LinearLayout>
            </LinearLayout>
            <ImageView android:id="@+id/image23986370311"
                android:layout_gravity="top|end"
                android:layout_height="@dimen/_56pxh"
                android:layout_marginBottom="@dimen/_57pxv"
                android:layout_marginEnd="@dimen/_8pxh"
                android:layout_marginStart="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_57pxv"
                android:layout_width="@dimen/_74pxh"
                android:scaleType="fitXY"
                imageRes="@{@drawable/img_23986370311}"
                tools:ignore="ContentDescription" tools:src="@drawable/img_23986370311"/>
            <ImageView android:id="@+id/image62ecb1b112644i"
                android:layout_gravity="bottom|start"
                android:layout_height="@dimen/_86pxh"
                android:layout_marginBottom="@dimen/_8pxv"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_10pxv"
                android:layout_width="@dimen/_87pxh"
                android:scaleType="fitXY"
                imageRes="@{@drawable/img_62ecb1b112644i}"
                tools:ignore="ContentDescription" tools:src="@drawable/img_62ecb1b112644i"/>
            <ImageView android:id="@+id/imagePngtransparentOne"
                android:layout_gravity="bottom|end"
                android:layout_height="@dimen/_49pxh"
                android:layout_marginBottom="@dimen/_28pxv"
                android:layout_marginStart="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_28pxv"
                android:layout_width="@dimen/_82pxh"
                android:scaleType="fitXY"
                imageRes="@{@drawable/img_pngtransparent}"
                tools:ignore="ContentDescription" tools:src="@drawable/img_pngtransparent"/>
        </FrameLayout>
    </LinearLayout>
</layout>
